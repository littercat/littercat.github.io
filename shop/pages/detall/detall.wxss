/*联系店铺*/
.bgcolor {
    background: rgba(0,0,0,.5);
    position: fixed;
    z-index: 100;
    height: 100%;
    width: 100%;
    left: 0;
    top: 0;
}
.storeInformation {
    position: fixed;
    top: 10%;
    left: 5%;
    z-index: 102;
    background: #fff;
    width: 90%;
    background-color: #fff;
    margin: 0 auto;
    border-radius: 7px;
    overflow: hidden;
    text-align: center;
    font-size: 14px;
    color: #ccc;
}
.storeInformation image{
  width:60px;
  height:60px;
}

.storeInformation .shopinfo {
    background: url('../../image/StoreBanner.png') no-repeat center;
    background-size: cover;
    border-top-right-radius: 7px;
    border-top-left-radius: 7px;
}

.storeInformation .shopinfo .info {
    min-height: 80px;
    padding: 15px !important;
    color:#666;
}

.storeInformation .shopinfo view text:nth-child(1) {
    position: relative;
    margin: auto;
    left: 0;
    right: 0;
    width: 60px;
    height: 60px;
    overflow: hidden;
    text-align: center;
    border: 1px solid #e6e6e6;
    margin-top: 10px;
}

.storeInformation .shopinfo view text:nth-child(1) img {
    width: 100%;
}

.storeInformation .shopinfo view text:nth-child(2) {
    text-align: center;
    margin-top: 10px;
    color: #fff;
}

.storeInformation .shopInforation {
    padding: 15px !important;
    text-align: center;
}

.storeInformation .shopInforation .list {
    border: 1px solid #e6e6e6;
    border-radius: 5px;
    margin-top: 10px !important;
    font-size: 14px;
    color:#666;
}

.storeInformation .shopInforation .listitem {
    padding: 10px;
    border-bottom: 1px solid #e6e6e6;
    line-height: 25px;
    display: flex;
}

.storeInformation .shopInforation .listitem view {
    width: calc(100% - 80px);
}

.storeInformation .shopInforation .listitem input {
    border: none;
    flex:1;
    width: 100%;
    text-align: left;
    padding-left:10px
}

.storeInformation .shopInforation .listitem text {
    width: 36px;
}

.storeInformation .shopInforation .listitem a {
    color: #FE6030;
    width: 40px;
}

.storeInformation .closeBtn {
    margin-bottom: 10px;
}

.storeInformation .closeBtn a {
    box-shadow: none;
    -webkit-transition: all 0.2s ease-in-out;
    -moz-transition: all 0.2s ease-in-out;
    transition: all 0.2s ease-in-out;
    margin: 5px 5px 5px 0;
    text-align: center;
    display: inline-block;
    border: none;
    background: #FE6030;
    color: #fff;
    font-weight: 400;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    border-radius: 3px;
    margin-top: 15px!important;
    padding: 5px 15px;
}

.storeInformation .shopInforation input {
    width: 100%;
}